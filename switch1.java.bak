class switch1 
{
	public static void main(String[] args) 
	{
		int a = 2;
		switch(a)
		{
			case 1:System.out.println("Inside the case 1");
			      break;
			case 2:System.out.println("Inside the case 2"):
				break;
            default :System.out.println("Inside the default case");
			    break;
		}
	}
}
